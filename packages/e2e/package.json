{
  "name": "e2e",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "tsc",
    "generate": "pnpm generate:session-manager",
    "generate:session-manager": "shx rm -rf src/generated/session-manager && gen-api-models --api-spec ../../apps/io-session-manager/api/external.yaml --out-dir src/generated/session-mananger --no-strict --request-types --response-decoders --client",
    "start": "node ./dist/index.js",
    "test:e2e": "vitest run"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "catalog:",
    "@pagopa/openapi-codegen-ts": "catalog:",
    "@pagopa/ts-commons": "catalog:",
    "@pagopa/typescript-config-node": "workspace:^",
    "@types/node-fetch": "catalog:",
    "eslint": "catalog:",
    "eslint-config-monorepo": "workspace:^",
    "eslint-plugin-prettier": "catalog:",
    "fp-ts": "^2.16.6",
    "io-ts": "catalog:",
    "node-fetch": "^2.6.1",
    "prettier": "catalog:",
    "shx": "^0.3.4",
    "typescript": "^5.4.3",
    "vitest": "catalog:"
  }
}
