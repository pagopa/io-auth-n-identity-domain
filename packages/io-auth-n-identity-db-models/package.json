{
  "name": "@pagopa/io-auth-n-identity-db-models",
  "version": "0.0.0",
  "license": "MIT",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "yarn generate:lollipop-definitions",
    "build": "yarn prebuild && tsc",
    "build:watch": "tsc -w",
    "generate:lollipop-definitions": "shx rm -rf src/generated/lollipop && gen-api-models --api-spec  https://raw.githubusercontent.com/pagopa/io-functions-commons/b057428d06dc170f75af2f9d9bb927bbb8ff7bd2/openapi/lollipop_definitions.yaml --out-dir src/generated/lollipop",
    "typecheck": "tsc --noemit",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@pagopa/openapi-codegen-ts": "^14.0.1",
    "@pagopa/typescript-config-node": "*",
    "@types/node": "^20.12.2",
    "@vitest/coverage-v8": "~3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-monorepo": "*",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "shx": "^0.4.0",
    "typescript": "^5.4.5",
    "vitest": "~3.2.4"
  },
  "dependencies": {
    "@azure/cosmos": "4.2.0",
    "@pagopa/ts-commons": "^13.1.2",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22"
  },
  "peerDependencies": {
    "@pagopa/ts-commons": "^13.1.2",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22"
  }
}
