{
  "name": "io-profile-async",
  "version": "1.17.1",
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "tsc && pnpm postbuild",
    "postbuild": "dependency-check package.json --no-dev --missing ./dist/**/*.js",
    "build:watch": "tsc -w",
    "typecheck": "tsc --noemit",
    "extensions:install": "func extensions install",
    "start": "dotenv -e .env func start --javascript",
    "start:watch": "pnpm start watch",
    "lint": "pnpm lint:check --fix",
    "lint:check": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-api": "find ./api -name '*.yaml' -exec swagger-cli validate {} \\;",
    "generate": "pnpm generate:definitions-sm-internal && pnpm generate:definitions-function-profile",
    "generate:definitions-sm-internal": "shx rm -rf ./src/generated/definitions/sm-internal && shx mkdir -p ./src/generated/definitions/sm-internal && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-auth-n-identity-domain/12cad284e0f3919c68b238933d57274c3deef978/apps/io-session-manager-internal/api/internal.yaml --no-strict --out-dir ./src/generated/definitions/sm-internal --request-types --response-decoders --client",
    "generate:definitions-function-profile": "shx rm -rf ./src/generated/definitions/function-profile && shx mkdir -p ./src/generated/definitions/function-profile && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-app/a42f14ae2c1d944c6c8d1ea6127a2b8f395396c8/openapi/index.yaml --no-strict --out-dir ./src/generated/definitions/function-profile --request-types --response-decoders --client",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "dist:modules": "modclean -r -n default:safe && pnpm install --production",
    "predeploy": "pnpm build && pnpm dist:modules"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.5.0",
    "@pagopa/eslint-config": "catalog:",
    "@pagopa/openapi-codegen-ts": "^14.0.0",
    "@types/html-to-text": "^9.0.4",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "date-fns": "catalog:",
    "dependency-check": "catalog:",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "eslint-config-monorepo": "workspace:^",
    "eslint-plugin-prettier": "catalog:",
    "fast-check": "catalog:",
    "modclean": "catalog:",
    "prettier": "catalog:",
    "shx": "^0.3.2",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@azure/cosmos": "catalog:",
    "@azure/data-tables": "catalog:",
    "@azure/storage-blob": "12.17.0",
    "@azure/storage-queue": "^12.16.0",
    "@pagopa/handler-kit": "^1.1.0",
    "@pagopa/handler-kit-azure-func": "^1.2.1",
    "@pagopa/io-app-email-templates": "catalog:",
    "@pagopa/io-auth-n-identity-commons": "workspace:^",
    "@pagopa/io-functions-commons": "catalog:",
    "@pagopa/logger": "catalog:",
    "@pagopa/ts-commons": "catalog:",
    "applicationinsights": "catalog:",
    "fp-ts": "catalog:",
    "html-to-text": "catalog:",
    "io-ts": "catalog:",
    "io-ts-types": "catalog:",
    "monocle-ts": "catalog:",
    "newtype-ts": "catalog:",
    "node-fetch": "catalog:",
    "nodemailer": "catalog:"
  }
}
