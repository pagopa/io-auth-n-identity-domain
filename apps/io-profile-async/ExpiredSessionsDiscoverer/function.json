{
  "bindings": [
    {
      "name": "expiredSessionsScannerTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 7 * * *"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 10,
    "minimumInterval": "00:00:05",
    "maximumInterval": "00:30:00"
  },
  "scriptFile": "../dist/main.js",
  "entryPoint": "ExpiredSessionsDiscoverer"
}
