{
  "name": "io-session-manager-internal",
  "version": "0.15.3",
  "author": "IO Auth & Identity Team",
  "main": "dist/controllers/main.js",
  "engines": {
    "node": ">= 20.0.0"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "clean:generated": "shx rm -rf ./src/generated",
    "build": "tsc && pnpm postbuild",
    "postbuild": "dependency-check package.json --no-dev --missing ./dist/**/*.js",
    "build:watch": "tsc -w",
    "typecheck": "tsc --noemit",
    "extensions:install": "func extensions install",
    "start": "dotenv -e .env -- func start --javascript",
    "start:watch": "pnpm start watch",
    "lint": "pnpm lint:check --fix",
    "lint:check": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "generate": "pnpm generate:definitions-internal && pnpm generate:definitions-types",
    "generate:definitions-internal": "shx rm -rf ./src/generated/definitions/internal && shx mkdir -p ./src/generated/definitions/internal && gen-api-models --api-spec ./api/internal.yaml --no-strict --out-dir ./src/generated/definitions/internal ",
    "generate:definitions-types": "shx rm -rf ./src/generated/definitions/types && shx mkdir -p ./src/generated/definitions/types && gen-api-models --api-spec ./api/definitions.yaml --no-strict --out-dir ./src/generated/definitions/types"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.2",
    "@azure/functions": "^4.7.0",
    "@azure/identity": "^4.10.0",
    "@azure/service-bus": "catalog:",
    "@azure/storage-blob": "^12.29.1",
    "@azure/storage-queue": "^12.16.0",
    "@pagopa/handler-kit": "^1.1.1",
    "@pagopa/handler-kit-azure-func": "^2.0.8",
    "@pagopa/io-auth-n-identity-commons": "workspace:^",
    "@pagopa/io-functions-commons": "^29.3.0",
    "@pagopa/logger": "catalog:",
    "@pagopa/ts-commons": "^13.1.2",
    "@redis/client": "1.6.1",
    "applicationinsights": "^2.9.5",
    "date-fns": "^1.30.1",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22",
    "io-ts-types": "^0.5.16",
    "monocle-ts": "catalog:",
    "newtype-ts": "catalog:",
    "redis": "4.7.1",
    "winston-transport": "catalog:"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "catalog:",
    "@pagopa/openapi-codegen-ts": "^13.2.0",
    "@pagopa/typescript-config-node": "workspace:^",
    "@types/node": "~20.19.5",
    "@vitest/coverage-v8": "catalog:",
    "dependency-check": "catalog:",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "eslint": "catalog:",
    "eslint-config-monorepo": "workspace:^",
    "eslint-plugin-prettier": "catalog:",
    "prettier": "catalog:",
    "shx": "^0.3.2",
    "typescript": "^5.4.3",
    "vitest": "catalog:"
  }
}
